{
    "version": 4,
    "configurePresets": [
      {
        "name": "debug",
        "displayName": "Debug and custom flags",
        "generator": "Ninja",
        "binaryDir": "${sourceDir}/build",
        "cacheVariables": {
          "CMAKE_BUILD_TYPE": "Debug",
          "CMAKE_C_COMPILER": "clang-17",
          "ENABLE_TRACY": "ON",
          "CMAKE_CXX_COMPILER": "clang++-17",
          "CMAKE_C_COMPILER_LAUNCHER": "ccache",
          "CMAKE_CXX_COMPILER_LAUNCHER": "ccache",
          "CMAKE_EXPORT_COMPILE_COMMANDS": "YES",
          "TTMLIR_ENABLE_EXPLORER": "ON",
          "TT_METAL_ENABLE_LOGGING": "ON",
          "TT_RUNTIME_DEBUG": "ON",
          "TT_RUNTIME_ENABLE_PERF_TRACE": "ON",
          "TTMLIR_BUILD_TYPE": "Debug",
          "TTMLIR_ENABLE_RUNTIME_TESTS": "ON",
          "TTMLIR_ENABLE_RUNTIME": "ON",
          "TTMLIR_ENABLE_OPMODEL": "ON",
          "TTMLIR_ENABLE_BINDINGS_PYTHON": "ON",
          "TTMETAL_ENABLE_PYTHON_BINDINGS": "ON",
          "TTMLIR_ENABLE_STABLEHLO": "ON"
        },
        "cmakeExecutable": "sea_cmake"
      },
      {
        "name": "release",
        "displayName": "Release and custom flags",
        "generator": "Ninja",
        "binaryDir": "${sourceDir}/build",
        "cacheVariables": {
          "CMAKE_BUILD_TYPE": "Release",
          "CMAKE_C_COMPILER": "clang-17",
          "CMAKE_CXX_COMPILER": "clang++-17",
          "CMAKE_C_COMPILER_LAUNCHER": "ccache",
          "CMAKE_CXX_COMPILER_LAUNCHER": "ccache",
          "CMAKE_EXPORT_COMPILE_COMMANDS": "YES",
          "TTMLIR_BUILD_TYPE": "Release",
          "TTMLIR_ENABLE_RUNTIME_TESTS": "ON",
          "TTMLIR_ENABLE_RUNTIME": "ON",
          "TTMLIR_ENABLE_BINDINGS_PYTHON": "ON",
          "TT_RUNTIME_DEBUG": "OFF",
          "TTMLIR_ENABLE_STABLEHLO": "ON"
        },
        "cmakeExecutable": "sea_cmake"
      }
    ]
}
